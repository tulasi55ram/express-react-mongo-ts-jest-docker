{
    "name": "mongoose-react-express-ts-jest",
    "version": "1.0.0",
    "description": "# Prerequisites",
    "main": "./dist/host.js",
    "scripts": {
        "start-dev": "tsc && copyfiles -f src/spec.yml dist && node dist/host.js",
        "generate-ts": "dtsgen --out src/models/types.d.ts src/spec.yml",
        "build": "tsc && npm run generate-ts && copyfiles -f src/spec.yml dist",
        "start": "node ./dist/host.js",
        "dev": "nodemon",
        "test": "jest --collect-coverage",
        "tslint:fix": "tslint --fix -c tslint.json -p tsconfig.json"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts, yml",
        "ignore": [
            "src/models/*.d.ts"
        ],
        "exec": "npm run build && npm run start"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:tulasi55ram/express-react-mongo-ts-jest-docker.git"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/multer": "^1.4.4",
        "bcryptjs": "^2.4.3",
        "config": "^3.1.0",
        "copyfiles": "^2.4.1",
        "express": "^4.16.4",
        "express-validator": "^5.3.1",
        "gravatar": "^1.8.0",
        "helmet": "^4.6.0",
        "http-status-codes": "^1.3.2",
        "js-yaml": "^3.14.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.5.7",
        "request": "^2.88.0",
        "tslint": "^6.1.3",
        "typescript": "^3.4.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/config": "0.0.34",
        "@types/express": "^4.16.1",
        "@types/gravatar": "^1.8.0",
        "@types/jest": "^26.0.15",
        "@types/js-yaml": "^3.12.5",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/mongoose": "^5.5.0",
        "@types/supertest": "^2.0.10",
        "@types/swagger-ui-express": "^4.1.2",
        "concurrently": "^4.1.0",
        "dotenv": "^8.2.0",
        "dtsgenerator": "^3.3.1",
        "express-openapi-validator": "^4.7.2",
        "jest": "^26.6.3",
        "nodemon": "^1.19.0",
        "supertest": "^6.0.1",
        "swagger-ui-express": "^4.1.5",
        "ts-jest": "^26.4.4"
    }
}
